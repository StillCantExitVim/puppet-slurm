
# PARTITIONS
<% 
@partitionlist.each do |part|
  partition = []
  [
    'PartitionName',
    'Nodes',
    'AllocNodes',
    'Default',
    'Priority',
    'PreemptMode',
    'Shared',
    'MaxNodes',
    'MinNodes',
    'DefaultTime',
    'MaxTime',
    'DefMemPerCPU',
    'MaxMemPerCPU',
    'DefMemPerNode',
    'MaxMemPerNode',
    'AllowGroups',
    'DisableRootJobs',
    'State',
  ].each do |key|
    partition << "#{key}=#{part[key]}" if part[key]
  end
-%>
<%= partition.join(" ") %>
<% end -%>


<% 
node = []
node << "NodeName=#{@hostname}"
node << "NodeAddr=#{@ipaddress}" if @ipaddress
node << "Sockets=#{@physicalprocessorcount}" if @physicalprocessorcount
node << "CoresPerSocket=#{@corecountpercpu}" if @corecountpercpu
node << "ThreadsPerCore=#{@threadcountpercore}" if @threadcountpercore
node << "Procs=#{@procs}" if @procs
node << "RealMemory=#{@real_memory}" if @real_memory
node << "TmpDisk=#{@tmp_disk}" if @tmp_disk
node << "State=UNKNOWN"
-%>
<%= node.join(" ") %>
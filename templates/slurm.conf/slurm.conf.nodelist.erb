<% 
node = []
node << "NodeName=#{@node_name}"
node << "NodeAddr=#{@node_addr}" if @node_addr and @node_addr != 'UNSET'
node << "CPUs=#{@cpus}" if @cpus and @cpus != 'UNSET'
node << "Sockets=#{@sockets}" if @sockets and @sockets != 'UNSET'
node << "CoresPerSocket=#{@cores_per_socket}" if @cores_per_socket and @cores_per_socket != 'UNSET'
node << "ThreadsPerCore=#{@threads_per_core}" if @threads_per_core and @threads_per_core != 'UNSET'
node << "RealMemory=#{@real_memory}" if @real_memory and @real_memory != 'UNSET'
node << "TmpDisk=#{@tmp_disk}" if @tmp_disk and @tmp_disk != 'UNSET'
node << "Feature=#{@feature}" if @feature and @feature != 'UNSET'
node << "State=#{@state}"
-%>
<%= node.join(" ") %>